@using Server.Infrastructure
@{
    var a = LibCollection.JsAngular;

    var aSant = LibCollection.JsAngularSantize;
    var aAnim = LibCollection.JsAngularAnimation;
    var aAria = LibCollection.JsAngularAria;
    var aMes = LibCollection.JsAngularMessages;
    var aMat = LibCollection.JsAngularMaterial;
    var aSvg = LibCollection.JsAngularSvgAssetCahce;
    var aFIleInput = LibCollection.JsAngularLfNgMdFileInput;
}

<script src="@a.CdnPathMin"
        asp-fallback-src="@a.LocalPathMin"
        asp-fallback-test="@a.FallbackTest"
        
        integrity="@a.IntegrityMin"
        crossorigin="anonymous"></script>

<script src="@aSant.CdnPathMin"
        asp-fallback-src="@aSant.LocalPathMin"
        asp-fallback-test="@aSant.FallbackTest"
        integrity="@aSant.IntegrityMin"
        crossorigin="anonymous"></script>
<script src="@aAnim.CdnPathMin"
        asp-fallback-src="@aAnim.LocalPathMin"
        asp-fallback-test="@aAnim.FallbackTest"
        integrity="@aAnim.IntegrityMin"
        crossorigin="anonymous"></script>
<script src="@aAria.CdnPathMin"
        asp-fallback-src="@aAria.LocalPathMin"
        asp-fallback-test="@aAria.FallbackTest"
        integrity="@aAria.IntegrityMin"
        crossorigin="anonymous"></script>

<script src="@aMes.CdnPathMin"
        asp-fallback-src="@aMes.LocalPathMin"
        asp-fallback-test="@aMes.FallbackTest"
        integrity="@aMes.IntegrityMin"
        crossorigin="anonymous"></script>
<script src="@aMat.CdnPathMin"
        asp-fallback-src="@aMat.LocalPathMin"
        asp-fallback-test="@aMat.FallbackTest"
        integrity="@aMat.IntegrityMin"
        crossorigin="anonymous"></script>
<script src="@aFIleInput.CdnPathMin"
        asp-fallback-src="@aFIleInput.LocalPathMin"
        asp-fallback-test="@aFIleInput.FallbackTest"
        integrity="@aFIleInput.IntegrityMin"
        crossorigin="anonymous"></script>

<script src="@aSvg.LocalPathMin" asp-append-version="@aSvg.AppendVersion"></script>
 

 
